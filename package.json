{
  "name": "covid-athenaeum",
  "version": "1.0.0",
  "description": "Information hub for COVID-19",
  "main": "app.js",
  "scripts": {
    "transpile": "babel ./server -d dist_server",
    "clean": "rimraf dist_server",
    "build": "npm-run-all clean transpile",
    "dev": "NODE_ENV=production nodemon server/app.js",
    "build-prod": "NODE_ENV=production npm-run-all build && cd client-app && npm run generate && cd ..",
    "start": "node dist_server/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanvirmislam/covid-athenaeum.git"
  },
  "keywords": [
    "data",
    "visualization",
    "learning",
    "information",
    "d3",
    "covid",
    "coronavirus",
    "awareness"
  ],
  "author": "Tanvir",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tanvirmislam/covid-athenaeum/issues"
  },
  "homepage": "https://github.com/tanvirmislam/covid-athenaeum#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongodb": "^3.5.5",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "nodemon": "^2.0.2"
  }
}
