{
  "name": "covid-athenaeum",
  "version": "1.0.0",
  "description": "Information hub for COVID-19",
  "main": "app.js",
  "scripts": {
    "lint": "eslint ./server/",
    "transpile": "babel ./server -d dist_server",
    "clean": "rimraf dist_server",
    "build-server": "npm-run-all lint clean transpile",
    "build-client": "cd client-app && npm run generate && cd ..",
    "build-prod": "npm-run-all build-server build-client",
    "dev": "NODE_ENV=development nodemon dist_server/app.js",
    "start": "node dist_server/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanvirmislam/covid-athenaeum.git"
  },
  "keywords": [
    "data",
    "visualization",
    "learning",
    "information",
    "d3",
    "covid",
    "coronavirus",
    "awareness"
  ],
  "author": "Tanvir",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tanvirmislam/covid-athenaeum/issues"
  },
  "homepage": "https://github.com/tanvirmislam/covid-athenaeum#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "csv-parser": "^2.3.2",
    "csv-writer": "^1.6.0",
    "express": "^4.17.1",
    "fast-csv": "^4.1.3",
    "mongodb": "^3.5.5",
    "neat-csv": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "objects-to-csv": "^1.3.6",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.2"
  }
}
