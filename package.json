{
  "name": "covid-athenaeum",
  "version": "1.0.0",
  "description": "Information hub for COVID-19",
  "main": "app.js",
  "scripts": {
    "lint": "eslint ./server/",
    "transpile": "babel ./server -d dist-build",
    "clean": "rimraf dist-build",
    "build-server": "npm-run-all lint clean transpile",
    "build-client": "cd client-app && npm run generate && cd ..",
    "build-prod": "npm-run-all build-server build-client",
    "dev": "NODE_ENV=development nodemon dist-build/app.js",
    "start": "node dist-build/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanvirmislam/covid-athenaeum.git"
  },
  "keywords": [
    "data",
    "visualization",
    "learning",
    "information",
    "d3",
    "covid",
    "coronavirus",
    "awareness"
  ],
  "author": "Tanvir",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tanvirmislam/covid-athenaeum/issues"
  },
  "homepage": "https://github.com/tanvirmislam/covid-athenaeum#readme",
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
